{
  "name": "moss-wall",
  "version": "1.0.0",
  "description": "It's just a wall of moss",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "git checkout -b deploy && webpack -p && git add -f public/bundle.js public/bundle.js.map && git commit --allow-empty -m 'Deploying' && git push --force heroku deploy:master && git checkout master && git branch -D deploy",
    "start": "node server",
    "start-dev": "npm run build-client-watch & npm run start-server",
    "start-server": "NODE_ENV='development' nodemon server -e html,js,scss --ignore public",
    "test": "NODE_ENV='test' mocha ./**/*.spec.js --compilers js:babel-register"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "body-parser": "^1.16.1",
    "express": "^4.14.1",
    "jsonfile": "^4.0.0",
    "morgan": "^1.8.1",
    "node-rest-client": "^3.1.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "request": "^2.82.0",
    "request-promise": "^4.2.2",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "enzyme": "^2.8.2",
    "meetup-api": "^1.4.23",
    "mocha": "^3.3.0",
    "react-test-renderer": "^15.6.1",
    "redux-mock-store": "^1.2.3",
    "supertest": "^3.0.0",
    "webpack": "^2.2.1"
  }
}
